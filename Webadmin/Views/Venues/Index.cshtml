@model IEnumerable<Webadmin.Models.Venues>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Venue Name
            </th>
            <th>
                Postcode
            </th>
            <th>
                Address Line One
            </th>
            <th>
                Address Line Two
            </th>
            <th>
                City
            </th>
            <th>
                County
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VenueName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VenuePostcode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddLineOne)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddLineTwo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.County)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VenueId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VenueId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VenueId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
