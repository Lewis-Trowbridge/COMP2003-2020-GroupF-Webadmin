            </div>
            <div class="form-group">
                <label asp-for="VenueTableCapacity" class="control-label"></label>
                <input asp-for="VenueTableCapacity" class="form-control" />
                <span asp-validation-for="VenueTableCapacity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
