@model Webadmin.Models.VenueTables

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>VenueTables</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
